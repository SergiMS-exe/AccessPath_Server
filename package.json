{
  "name": "access_path_server",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "npm": ">=9.0.0 <=9.5.1",
    "node": ">=16.0.0 <19.0.0"
  },
  "scripts": {
    "dev": "nodemon ./src/config/server.ts",
    "test": "jest --forceExit --detectOpenHandles",
    "test:unit": "jest ./__tests__/unit/**/*.test.ts",
    "test:integration": "cross-env NODE_ENV=test jest ./__tests__/integration/**/*.test.ts",
    "build": "rimraf dist && tsc",
    "add-build": "git add dist",
    "start": "node ./dist/config/server.js"
  },
  "pre-commit": [
    "build",
    "add-build"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parse": "0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongo-sanitize": "1.1.0",
    "mongodb": "^5.5.0",
    "mongoose": "^7.3.1",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "jest": "^29.6.1",
    "jest-express": "^1.12.0",
    "mongodb-memory-server": "^9.0.1",
    "nodemon": "^3.0.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}